<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <managed-bean>
  <managed-bean-name>yoneticiView</managed-bean-name>
  <managed-bean-class>yonetici.YoneticiView</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>uyeView</managed-bean-name>
  <managed-bean-class>uye.UyeView</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>aracView</managed-bean-name>
  <managed-bean-class>arac.AracView</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>rezervasyonView</managed-bean-name>
  <managed-bean-class>rezervasyon.RezervasyonView</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>imagesView</managed-bean-name>
  <managed-bean-class>images.ImagesView</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/yoneticigiris.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>yoneticipanel</from-outcome>
   <to-view-id>/yoneticipanel.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/uyegiris.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>index</from-outcome>
   <to-view-id>/index.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/yoneticipanel.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>yoneticigiris</from-outcome>
   <to-view-id>/yoneticigiris.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/uyelerpanel.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>yoneticigiris</from-outcome>
   <to-view-id>/yoneticigiris.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/araclarpanel.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>yoneticigiris</from-outcome>
   <to-view-id>/yoneticigiris.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application/>
</faces-config>
